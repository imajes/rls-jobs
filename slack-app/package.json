{
  "name": "rls-jobs-slack-app",
  "version": "1.0.0",
  "description": "Structured job and candidate intake Slack app for RLS",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:prod": "node app.js",
    "test": "node --test test/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage test/*.test.js",
    "test:coverage:check": "node --test --experimental-test-coverage --test-coverage-lines=85 --test-coverage-functions=85 --test-coverage-branches=60 test/*.test.js",
    "lint": "npx @biomejs/biome check .",
    "lint:fix": "npx @biomejs/biome check --write .",
    "export:ui": "node scripts/export-ui-prototypes.js",
    "check:deploy": "node scripts/check-deploy-readiness.js --strict"
  },
  "author": "RLS Jobs",
  "license": "MIT",
  "keywords": [
    "slack",
    "bolt",
    "jobs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/example/rls-jobs.git"
  },
  "bugs": {
    "url": "https://github.com/example/rls-jobs/issues"
  },
  "dependencies": {
    "@slack/bolt": "4.6.0",
    "dotenv": "~17.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@slack/cli-hooks": "^1.3.0"
  }
}
